<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="booking.domain">

    <class name="Booking" table="BOOKING">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <many-to-one name="user" class="booking.domain.User" column="USER" fetch="join" lazy="false"/>
        <many-to-one name="ticket" class="booking.domain.Ticket" column="TICKET" fetch="join" lazy="false"/>
    </class>

</hibernate-mapping>